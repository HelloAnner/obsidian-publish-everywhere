{
	"name": "obsidian-publish-everywhere",
	"version": "1.0.0",
	"description": "Publish Obsidian notes to KMS (Confluence) directly from Obsidian",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "node node_modules/typescript/lib/tsc.js -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"kms",
		"publish"
	],
	"author": "Anner Wang",
	"license": "MIT",
	"devDependencies": {
		"@types/mime-types": "^3.0.1",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"@notionhq/client": "^2.2.14",
		"image-size": "^2.0.2",
		"mdast-util-to-string": "^4.0.0",
		"mime-types": "^2.1.35",
		"playwright-core": "1.49.1",
		"rehype-raw": "^7.0.0",
		"rehype-stringify": "^10.0.0",
		"remark-gfm": "^4.0.0",
		"remark-parse": "^11.0.0",
		"remark-rehype": "^11.1.1",
		"unified": "^11.0.5",
		"unist-util-visit": "^5.0.0"
	}
}
